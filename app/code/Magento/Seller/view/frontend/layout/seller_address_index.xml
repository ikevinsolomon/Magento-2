<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="seller_account" />
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="seller_address_head_components" template="Magento_Seller::js/components.phtml" />
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Seller\Block\Address\Book" name="address_book" template="Magento_Seller::address/book.phtml" cacheable="false" />
            <block class="Magento\Seller\Block\Address\Grid" name="address_grid" template="Magento_Seller::address/grid.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>