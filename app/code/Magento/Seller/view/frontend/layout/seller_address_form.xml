<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="seller_account" />
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="seller_page_head_components" template="Magento_Seller::js/components.phtml" />
        </referenceBlock>
        <referenceBlock name="seller-account-navigation-address-link">
            <arguments>
                <argument name="is_highlighted" xsi:type="boolean">true</argument>
            </arguments>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Seller\Block\Address\Edit" name="seller_address_edit" template="Magento_Seller::address/edit.phtml" cacheable="false">
                <arguments>
                    <argument name="attribute_data" xsi:type="object">Magento\Seller\Block\DataProviders\AddressAttributeData</argument>
                    <argument name="post_code_config" xsi:type="object">Magento\Seller\Block\DataProviders\PostCodesPatternsAttributeData</argument>
                    <argument name="view_model" xsi:type="object">Magento\Seller\ViewModel\Address</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>