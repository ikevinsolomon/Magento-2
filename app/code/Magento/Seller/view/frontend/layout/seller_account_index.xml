<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="seller_account" />
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Account</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Seller\Block\Account\Dashboard\Info" name="seller_account_dashboard_info" as="info" template="Magento_Seller::account/dashboard/info.phtml" cacheable="false">
                <container name="seller.account.dashboard.info.blocks" as="additional_blocks" />
            </block>
            <block class="Magento\Seller\Block\Account\Dashboard\Address" name="seller_account_dashboard_address" as="address" template="Magento_Seller::account/dashboard/address.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>